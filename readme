clang -S \
    -target bpf \
    -D __BPF_TRACING__ \
    -Wall \
    -Wno-unused-value \
    -Wno-pointer-sign \
    -Wno-compare-distinct-pointer-types \
    -Werror \
    -O2 -emit-llvm -c -g -o kernel.ll kernel.c
llc -march=bpf -filetype=obj -o kernel.o kernel.ll

clang -target bpf -c af_xdp_kern.c  -o af_xdp_kern.o -O2 -llibbpf
ip link set dev enp161s0f1 xdp off
 ip link set dev enp161s0f0 xdp obj kernel.o sec xdp_sock

 ip link set dev enp161s0f0 xdp obj af_xdp_kern.o sec xdp_sock
 cat /sys/kernel/debug/tracing/trace_pipe
 
cmake -DCMAKE_BUILD_TYPE=Release ..  
-s 0 -i enp161s0f0 -f /root/sra/xsk_srv6/kernel.o -q 16 -m 1 -e 0 -s 1 -i enp161s0f1 -f /root/sra/xsk_srv6/kernel.o -q 16 -m 1 -e 1 -c 15
